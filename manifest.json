{

  "manifest_version": 3,
  "name": "SorareStats",
  "author": "Eddy Lafon",
  "version": "1.1.0.5",
  "host_permissions": [
    "*://*.soraredata.com/*",
    "*://*.lafoneddy.eu/*"
  ],
  "description": "Add link soraredata from player page",  
  "icons": {
    "48": "icons/soraredata_logo.png"
  },  
  "content_scripts": [
    {
      "matches": ["*://*.sorare.com/*", "*://*.soraredata.com/*"],
      "js": ["soraredata_sorare.js"]
    }
  ]

}